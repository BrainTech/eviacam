#: src/wconfiguration.h:89
msgid "Configuration"
msgstr "Configuració"

#: src/wconfiguration.cpp:329
msgid "X axis speed"
msgstr "Velocitat eix X"

#: src/wconfiguration.cpp:334
msgid "Sets response sensitivity for the X axis."
msgstr ""
"Configura la sensibilitat del\n"
"moviment per l'eix X."

#: src/wconfiguration.cpp:337
msgid "Y axis speed"
msgstr "Velocitat eix Y"

#: src/wconfiguration.cpp:342
msgid "Sets response sensitivity for the Y axis."
msgstr ""
"Configura la sensibilitat del\n"
"moviment per l'eix Y."

#: src/wconfiguration.cpp:345
msgid "Acceleration"
msgstr "Acceleració"

#: src/wconfiguration.cpp:350
msgid ""
"Sets pointer's acceleration.\n"
"0 means no acceleration and\n"
"5 maximum acceleration."
msgstr ""
"Configura l'acceleració del punter.\n"
"0 desactiva l'acceleració i\n"
"5 configura l'acceleració màxima."

#: src/wconfiguration.cpp:353
msgid "Smoothness"
msgstr "Suavitzat"

#: src/wconfiguration.cpp:358
msgid ""
"Tremor reduction filter.\n"
"0 means no filter and \n"
"9 maximum filtering."
msgstr ""
"Filtre per reduir tremolors\n"
"0 desactiva el filtre i\n"
"9 configura el filtrat màxim."

#: src/wconfiguration.cpp:361
msgid "Easy stop"
msgstr "Aturada fàcil"

#: src/wconfiguration.cpp:366
msgid ""
"Minimum displacement (in pixels)\n"
"to start moving pointer."
msgstr ""
"Desplaçament mínim (en píxels) abans\n"
"de començar a moure el punter."

#: src/wconfiguration.cpp:369
msgid "Enabled at startup"
msgstr "Activat a l'engegar"

#: src/wconfiguration.cpp:372
msgid ""
"If checked the program automatically \n"
"enables cursor control at startup."
msgstr ""
"Si està seleccionat activa el control del\n"
"punter a l'engegar el programa."

#: src/wconfiguration.cpp:375
msgid "Motion"
msgstr "Moviment"

#: src/wconfiguration.cpp:381
msgid "Use workspace limit"
msgstr "Limitar àrea de treball"

#: src/wconfiguration.cpp:384
msgid ""
"Confines the mouse pointer\n"
"to the selected area."
msgstr ""
"Confina el punter del ratolí\n"
"dins l'àrea seleccionada."

#: src/wconfiguration.cpp:391
msgid "Top"
msgstr "Dalt"

#: src/wconfiguration.cpp:396
msgid "Top limit workspace."
msgstr "Limit superior."

#: src/wconfiguration.cpp:400 src/wconfiguration.cpp:416
#: src/wconfiguration.cpp:430 src/wconfiguration.cpp:444
msgid "%"
msgstr "%"

#: src/wconfiguration.cpp:407 src/clickwindowbitmap.cpp:229
msgid "Left"
msgstr "Esquerra"

#: src/wconfiguration.cpp:412
msgid "Left limit workspace."
msgstr "Límit esquerre."

#: src/wconfiguration.cpp:421 src/clickwindowbitmap.cpp:236
msgid "Right"
msgstr "Dreta"

#: src/wconfiguration.cpp:426
msgid "Right limit workspace."
msgstr "Límit dret."

#: src/wconfiguration.cpp:435
msgid "Bottom"
msgstr "Baix"

#: src/wconfiguration.cpp:440
msgid "Bottom limit workspace."
msgstr "Límit inferior."

#: src/wconfiguration.cpp:447
msgid "Workspace"
msgstr "Àrea de treball"

#: src/wconfiguration.cpp:453
msgid "Dwell click"
msgstr "Clic per espera"

#: src/wconfiguration.cpp:458
msgid "Dwell time (ds)"
msgstr "Temps d'espera (ds)"

#: src/wconfiguration.cpp:463
msgid ""
"Time to wait (deciseconds) \n"
"before sending a click."
msgstr ""
"Temps d'espera (dècimes de segon)\n"
"abans de generar un clic."

#: src/wconfiguration.cpp:466
msgid "Dwell area"
msgstr "Àrea de clic"

#: src/wconfiguration.cpp:471
msgid ""
"Maximum allowed displacement\n"
"before restarting the dwell time\n"
"countdown."
msgstr ""
"Màxim desplaçament permès\n"
"abans de re-iniciar el compte\n"
"enrere del temps d'espera."

#: src/wconfiguration.cpp:474
msgid "Allow consecutive clicks"
msgstr "Permet clics consecutius"

#: src/wconfiguration.cpp:477
msgid ""
"Allows to send multiple clicks\n"
"when the pointer is stopped."
msgstr ""
"Permet enviar múltiples clics\n"
"quan el punter està aturat."

#: src/wconfiguration.cpp:480
msgid "Beep on click"
msgstr "Reproduir so amb el clic"

#: src/wconfiguration.cpp:483
msgid "Play sound when click generated."
msgstr "Reprodueix un so al generar un clic."

#: src/wconfiguration.cpp:486
msgid "Show click countdown"
msgstr "Mostrar compte enrere"

#: src/wconfiguration.cpp:491
msgid "Dwell click enabled"
msgstr "Clic per espera activat"

#: src/wconfiguration.cpp:494
msgid ""
"Enable/Disable automatic (dwell)\n"
"click generation mechanism."
msgstr ""
"Activa/Desactiva el mecanisme\n"
"de generació de clic per espera."

#: src/wconfiguration.cpp:497
msgid "Click window"
msgstr "Finestra del Clic"

#: src/wconfiguration.cpp:500
msgid "Open Click Window at startup"
msgstr "Obrir la Finestra del Clic a l'engegar."

#: src/wconfiguration.cpp:503
msgid ""
"If checked the Click Window is automatically\n"
"opened at program startup."
msgstr ""
"Si està activat la Finestra del Clic s'obre\n"
"automàticament a l'engegar el programa."

#: src/wconfiguration.cpp:506
msgid "Show Click Window"
msgstr "Mostrar la Finestra del Clic"

#: src/wconfiguration.cpp:512
msgid "Design:"
msgstr "Diseny"

#: src/wconfiguration.cpp:516
msgid "Normal"
msgstr "Normal"

#: src/wconfiguration.cpp:517
msgid "Thin"
msgstr "Fi"

#: src/wconfiguration.cpp:521
msgid "Behaviour:"
msgstr "Comportament"

#: src/wconfiguration.cpp:525
msgid "Normal mode"
msgstr "Mode normal"

#: src/wconfiguration.cpp:526
msgid "Fast mode"
msgstr "Mode ràpid"

#: src/wconfiguration.cpp:529
msgid ""
"Fast mode enables click type\n"
"selection by hovering the mouse\n"
"pointer over the click window\n"
"buttons."
msgstr ""
"El mode ràpid permet escollir el tipus de\n"
"clic passant el punter del ratolí per sobre\n"
"dels botons de la finestra del clic."

#: src/wconfiguration.cpp:532
msgid "Click"
msgstr "Clic"

#: src/wconfiguration.cpp:538
msgid "Face localization"
msgstr "Localització facial"

#: src/wconfiguration.cpp:541
msgid "Locate face automatically"
msgstr "Localitzar rostre automàticament"

#: src/wconfiguration.cpp:544
msgid ""
"When enabled tries to automatically\n"
"detect your face and center the\n"
"tracking area."
msgstr ""
"Quan s'activa intenta detectar el seu\n"
"rostre i centrar l'àrea de seguiment\n"
"de forma automàtica."

#: src/wconfiguration.cpp:547
msgid "Show locate face filter"
msgstr "Mostrar filtre de localització de rostre"

#: src/wconfiguration.cpp:552
msgid "On-screen keyboard command"
msgstr "Comanda del teclat en pantalla"

#: src/wconfiguration.cpp:557
msgid "Command to run an external on-screen keyboard application."
msgstr "Comanda per executar un programa extern de teclat en pantalla."

#: src/wconfiguration.cpp:560
msgid "Browse..."
msgstr "Cercar..."

#: src/wconfiguration.cpp:564 src/activationkey.h:52
msgid "Activation key"
msgstr "Tecla d'activació"

#: src/wconfiguration.cpp:568
msgid "Use activation key"
msgstr "Usar la tecla d'activació"

#: src/wconfiguration.cpp:571
msgid ""
"When checked it allows to enable or \n"
"disable eViacam by pressing a key."
msgstr ""
"Si està seleccionat permet activar o\n"
"desactivar eViacam prement una tecla."

#: src/wconfiguration.cpp:578
msgid "Shows the selected key."
msgstr "Mostra la tecla seleccionada."

#: src/wconfiguration.cpp:584
msgid "Set key"
msgstr "Establir tecla"

#: src/wconfiguration.cpp:590
msgid "Advanced"
msgstr "Avançat"

#: src/wconfiguration.cpp:592
msgid "Profile options"
msgstr "Opcions del perfil"

#: src/wconfiguration.cpp:599
msgid "Run at startup"
msgstr "Executar al iniciar el sistema"

#: src/wconfiguration.cpp:602
msgid "Start eViacam at user logon"
msgstr "Executar eViacam al iniciar la seva sessió"

#: src/wconfiguration.cpp:608
msgid "Profile management"
msgstr "Gestió de perfils"

#: src/wconfiguration.cpp:613
msgid "Select profile:"
msgstr "Escollir perfil"

#: src/wconfiguration.cpp:615 src/wconfiguration.cpp:621
msgid "Chooses desired active profile"
msgstr "Permet escollir el perfil actiu"

#: src/wconfiguration.cpp:626 src/wconfiguration.cpp:1174
msgid "Add profile"
msgstr "Nou perfil"

#: src/wconfiguration.cpp:629
msgid "Delete profile"
msgstr "Esborrar perfil"

#: src/wconfiguration.cpp:632
msgid "Selected camera"
msgstr "Càmera seleccionada"

#: src/wconfiguration.cpp:643
msgid "Settings..."
msgstr "Opcions..."

#: src/wconfiguration.cpp:646
msgid "Change"
msgstr "Canviar"

#: src/wconfiguration.cpp:651
msgid "Language:"
msgstr "Idioma:"

#: src/wconfiguration.cpp:658
msgid "Run at system startup"
msgstr "Executar a l'iniciar el sistema"

#: src/wconfiguration.cpp:663
msgid "General options"
msgstr "Opcions generals"

#: src/wconfiguration.cpp:670
msgid "&OK"
msgstr "&Acceptar"

#: src/wconfiguration.cpp:673
msgid "&Cancel"
msgstr "&Cancel·lar"

#: src/wconfiguration.cpp:869
msgid "Discard changes?"
msgstr "Descartar canvis?"

#: src/wconfiguration.cpp:869 src/wconfiguration.cpp:1347
#: src/viacamcontroller.cpp:369
msgid "eViacam warning"
msgstr "Advertiment"

#: src/wconfiguration.cpp:970
msgid ""
"This action will disable eViacam click generation.\n"
"Are you sure?"
msgstr ""
"Aquesta acció desactivarà la generació automàtica de clics.\n"
"Està segur?"

#: src/wconfiguration.cpp:1097 src/wconfiguration.cpp:1277
msgid "You should restart the application to apply this change"
msgstr "Cal que re-iniciï l'aplicació per a que aquest canvi tingui efecte."

#: src/wconfiguration.cpp:1118
msgid ""
"Current profile has unsaved changes.\n"
"Continue anyway?"
msgstr ""
"No s'han guardat els canvis del perfil actual\n"
"Continuar sense guardar?"

#: src/wconfiguration.cpp:1124
#, c-format
msgid ""
"This action will load %s profile settings.\n"
"Are you sure you want to continue?"
msgstr ""
"Aquesta acció carregarà el perfil %s.\n"
"Està segur que vol continuar?"

#: src/wconfiguration.cpp:1155
#, c-format
msgid ""
"This action will delete %s profile.\n"
"Are you sure you want to continue?"
msgstr ""
"Aquesta acció esborrarà el perfil %s.\n"
"Està segur que vol continuar?"

#: src/wconfiguration.cpp:1174
msgid "Enter new profile name"
msgstr "Nom del nou perfil"

#: src/wconfiguration.cpp:1185
msgid "Profile already exists."
msgstr "El perfil ja existeix."

#: src/wconfiguration.cpp:1191
msgid "Invalid profile name."
msgstr "Nom de perfil invàlid."

#: src/wconfiguration.cpp:1231 src/wconfiguration.cpp:1233
msgid "Choose on-screen keyboard command"
msgstr "Escolleixi la comanda del teclat en pantalla"

#: src/wconfiguration.cpp:1231
msgid "Executable files|*.exe"
msgstr "Arxius executables|*.exe"

#: src/wconfiguration.cpp:1347
msgid ""
"This action could make you lose the control of the computer.\n"
"Do you want to continue?"
msgstr ""
"Aquesta acció li pot fer perdre el control de l'ordinador.\n"
"Està segur que vol continuar?"

