#: src/wwizard.cpp:1419
msgid "Camera setup"
msgstr "Configuración de la cámara"

#: src/wwizard.cpp:1428
msgid "Checking your camera settings"
msgstr "Comprobación de los ajustes de su cámara"

#: src/wwizard.cpp:1437
msgid "Detected camera:"
msgstr "Cámara detectada:"

#: src/wwizard.cpp:1440
msgid "camera_name"
msgstr "camera_name"

#: src/wwizard.cpp:1444
msgid "Framerate:"
